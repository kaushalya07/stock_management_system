<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.stock.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>





	<servlet>
		<servlet-name>supplierinsert</servlet-name>
		<servlet-class>com.stock.supplierinsert</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>supplierinsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>stocklist</servlet-name>
		<servlet-class>com.stock.stocklistservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stocklist</servlet-name>
		<url-pattern>/stocklist</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>updatestock</servlet-name>
		<servlet-class>com.stock.updatestockservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatestock</servlet-name>
		<url-pattern>/updatestock</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>supplier</servlet-name>
		<servlet-class>com.stock.supplierservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>supplier</servlet-name>
		<url-pattern>/supplier</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>updatesupplier</servlet-name>
		<servlet-class>com.stock.updatesupplierservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>updatesupplier</servlet-name>
		<url-pattern>/updatesupplier</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deletesupplier</servlet-name>
		<servlet-class>com.stock.deletesupplierservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deletesupplier</servlet-name>
		<url-pattern>/deletesupplier</url-pattern>
	</servlet-mapping>



</web-app>